{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "development",
        "EXPO_PUBLIC_API_URL": "http://localhost:8080/api",
        "EXPO_PUBLIC_SOCKET_URL": "http://localhost:8080",
        "EXPO_PUBLIC_SENTRY_DSN": ""
      }
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_API_URL": "https://staging-api.lmsapp.com",
        "EXPO_PUBLIC_SOCKET_URL": "https://staging-api.lmsapp.com",
        "EXPO_PUBLIC_SENTRY_DSN": ""
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_API_URL": "https://api.lmsapp.com",
        "EXPO_PUBLIC_SOCKET_URL": "https://api.lmsapp.com",
        "EXPO_PUBLIC_SENTRY_DSN": ""
      }
    }
  }
}
